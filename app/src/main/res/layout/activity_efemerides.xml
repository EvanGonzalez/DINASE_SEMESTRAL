<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".efemerides">

    <TextView
        android:id="@+id/titulo"
        android:layout_width="409dp"
        android:layout_height="30dp"
        android:text="Tipo de Efemérides"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="29dp" />

    <TextView
        android:id="@+id/titulo2"
        android:layout_width="410dp"
        android:layout_height="39dp"
        android:text="Objeto"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="150dp" />

    <TextView
        android:id="@+id/titulo3"
        android:layout_width="409dp"
        android:layout_height="51dp"
        android:text="localización del Observador"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="271dp" />

    <TextView
        android:id="@+id/titulo4"
        android:layout_width="409dp"
        android:layout_height="51dp"
        android:text="Fechas"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="408dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="369dp"
        android:layout_height="61dp"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="74dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="342dp"
        android:layout_height="56dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="189dp" />

    <Button
        android:id="@+id/map"
        android:layout_width="304dp"
        android:layout_height="64dp"
        android:text="Google Map"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="333dp" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="470dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="216dp"
        android:layout_height="71dp"
        android:text="Generate Ephemeries"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="613dp" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="539dp" />
</androidx.constraintlayout.widget.ConstraintLayout>